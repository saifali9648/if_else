#include<iostream>
using namespace std;
int main()
{
	char ch;
	cout<<"enter a character or number:-"<<endl;
	cin>>ch;
	if(ch>=65 && ch<=90)
	{
		cout<<"under the uppar case"<<endl;
	}
	else if(ch>=97 && ch<=122)
	{
		cout<<"under the lower case"<<endl;
	}
	else
	{
		cout<<"this is numeric"<<ch<<endl;
	}
return 0;
}
